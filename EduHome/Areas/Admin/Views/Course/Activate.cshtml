@{
    ViewData["Title"] = "Activate";
}
<div class="row">
    <div class="col-lg-6 grid-margin stretch-card">
        <div class="card">
            <div class="card-body">
                <h3 class="text-capitalize"> Are you sure activate this Course ?</h3>
                <form method="post">

                    <button type="submit" class="btn btn-primary">Yes</button>
                    <a asp-action="Index" class="btn btn-primary">Go Back</a>

                </form>

            </div>
        </div>
    </div>
</div>
